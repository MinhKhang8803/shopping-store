{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "plugin:react/recommended",
    "overrides": [
        {
            "env": {
                "node": true
            },
            "files": [
                ".eslintrc"
            ],
            "parserOptions": {
                "sourceType": "module"
            }
        }
    ],
    
    "rules": {
        "react/prop-types": "off"
      },
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "ecmaVersion": "latest"
},

    "plugins": [
        "react"
    ]

    
}
